{
  "name": "micro-blog",
  "version": "1.0.0",
  "description": "Micro-blog with Angular4/Electron/Express-PouchDB",
  "homepage": "https://github.com/flamusdiu/microblog",
  "author": {
    "name": "Jesse Spangenberger",
    "email": "flamsdiu@gmail.com"
  },
  "keywords": [
    "angular",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "webpack --watch",
    "test": "karma start ./karma.conf.js",
    "lint": "ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "build": "webpack && copyfiles main.js start-electron.js dist",
    "prepree2e": "npm start",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "electron:serve": "electron . --serve",
    "electron:dist": "npm run build && electron dist/main.js",
    "electron:linux": "npm run build && copyfiles package.json dist && electron-packager dist --overwrite --platform=linux --arch=x64 --asar=true  --out=app-builds",
    "electron:windows": "npm run build && copyfiles package.json dist && electron-packager dist --overwrite --platform=win32 --arch=ia32 --asar=false --out=app-builds",
    "electron:mac": "npm run build && copyfiles package.json dist && electron-packager . --overwrite --platform=darwin --arch=x64 --asar=true --out=app-builds",
    "postinstall": "electron-rebuild"
  },
  "dependencies": {
    "@angular/animations": "^4.1.0",
    "@angular/common": "^4.1.0",
    "@angular/compiler": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/forms": "^4.1.0",
    "@angular/http": "^4.1.0",
    "@angular/material": "^2.0.0-beta.3",
    "@angular/platform-browser": "^4.1.0",
    "@angular/platform-browser-dynamic": "^4.1.0",
    "@angular/router": "^4.1.0",
    "angular2-markdown": "^1.6.0",
    "bluebird": "^3.5.0",
    "cluster": "^0.7.7",
    "core-js": "~2.4.1",
    "express": "^4.15.2",
    "express-pouchdb": "^2.3.6",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "pouchdb": "^6.2.0",
    "rxjs": "^5.3.0",
    "zone.js": "~0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.1",
    "@angular/compiler-cli": "^4.1.0",
    "@types/electron": "~1.4.35",
    "@types/jasmine": "~2.5.38",
    "@types/node": "~7.0.14",
    "autoprefixer": "^6.7.7",
    "codelyzer": "^3.0.1",
    "copyfiles": "~1.2.0",
    "css-loader": "^0.28.0",
    "cssnano": "~3.10.0",
    "electron": "1.6.6",
    "electron-packager": "~8.6.0",
    "electron-rebuild": "^1.5.7",
    "electron-reload": "~1.1.0",
    "exports-loader": "~0.6.3",
    "file-loader": "^0.11.1",
    "istanbul-instrumenter-loader": "~2.0.0",
    "jasmine-core": "^2.6.1",
    "jasmine-spec-reporter": "^4.1.0",
    "json-loader": "~0.5.4",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "~0.2.2",
    "karma-sourcemap-loader": "~0.3.7",
    "less-loader": "^4.0.3",
    "postcss-loader": "^1.3.3",
    "postcss-url": "^6.0.4",
    "protractor": "~5.1.0",
    "raw-loader": "~0.5.1",
    "sass-loader": "^6.0.3",
    "script-loader": "~0.7.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.16.1",
    "stylus-loader": "^3.0.1",
    "ts-node": "^3.0.2",
    "tslint": "^5.1.0",
    "typescript": "~2.3.0",
    "url-loader": "~0.5.7",
    "webdriver-manager": "^12.0.4",
    "webpack": "^2.4.1"
  },
  "license": "SEE LICENSE IN LICENSE.md"
}
